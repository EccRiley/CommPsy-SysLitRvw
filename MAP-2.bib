@article{taft2014strength,
author={Taft,Casey T. and Howard,Jamie and Monson,Candice M. and Walling,Sherry M. and Resick,Patricia A. and Murphy,Christopher M.},
    year={2014},
    title={"Strength at Home" intervention to prevent conflict and violence in military couples: Pilot findings},
    journal={Partner Abuse},
    volume={5},
    number={1},
    pages={41-57},
    abstract={In this article, we report on a pilot study of Strength at Home-Couples ( SAH-C), a 10-session cognitive-behavioral couples-based group intervention designed to prevent intimate partner violence (IPV) in military couples. The primary purposes of this pilot study were to determine feasibility of recruiting, retaining, and assessing SAH-C participants in addition to those participating in a comparison Supportive Therapy ( ST) group-based couples intervention. Recruitment was challenging for this pilot study and we report on several barriers to recruitment as well as "lessons learned" for enhancing recruitment and overall intervention efforts. Preliminary pilot data were promising with respect to reductions and prevention of IPV in those receiving the SAH-C intervention. Initial results for the secondary intervention targets were less favorable for the SAH-C intervention, with effect sizes suggesting a trend in which relationship satisfaction increased more in the ST intervention. (PsycINFO Database Record (c) 2016 APA, all rights reserved) (Source: journal abstract)},
    keywords={military couples; Strength at Home Couples intervention; cognitive-behavioral couples-based group intervention; intimate partner violence; Empirical Study; Quantitative Study; Human; Male; Female; Adulthood (18 yrs & older); Couples; Group Intervention; Cognitive Behavior Therapy; Military Personnel; Couples Therapy; US; article; 3313:Group & Family Therapy; 3800:Military Psychology},
}

@article{branch2013exploratory,
    author={Branch,Kathryn A. and Richards,Tara N. and Dretsch,Elizabeth C.},
    year={2013},
    title={An exploratory analysis of college students' response and reporting behavior regarding intimate partner violence victimization and perpetration among their friends},
    journal={Journal of Interpersonal Violence},
    volume={28},
    number={18},
    pages={3386-3399},
    abstract={Over the last several decades, an extensive literature has documented the prevalence of dating violence on college campuses. As a result, initiatives to promote awareness of dating violence on college campuses have proliferated and models of "bystander intervention" have been developed. Bystander intervention asserts that by giving all students information concerning the prevalence and warning signs of dating violence, these individuals will "say something" when they see problem behavior. However, a paucity of empirical research exists concerning whether students actually report their observation of dating violence among their peers. In the present study, a sample of college freshman ( n = 275) was surveyed regarding their willingness to report dating violence behaviors among their friends. Findings indicate that a slight majority of respondents report that they would report dating violence victimization among their friends to university officials (54%) and/or law enforcement (56%) while fewer indicated they would report perpetration, 38% and 42%, respectively. Importantly, respondents overwhelmingly report that they would attempt to intervene in the dating violence victimization (87%) and perpetration (84%). Implications of these findings for campus safety initiatives and bystander education models are discussed. (PsycINFO Database Record (c) 2016 APA, all rights reserved) (Source: journal abstract)},
    keywords={college students; intimate partner violence; victimization; reporting behavior; bystander intervention; dating violence; college campuses; Empirical Study; Longitudinal Study; Prospective Study; Quantitative Study; Human; Male; Female; Adolescence (13-17 yrs); Adulthood (18 yrs & older); Young Adulthood (18-29 yrs); Social Dating; Abuse Reporting; Bystander Effect; Campuses; US; article; 3230:Behavior Disorders & Antisocial Behavior; Young Adult; Humans; Interpersonal Relations; Helping Behavior; Universities; Students; Friends; Adolescent; Sex Offenses},
